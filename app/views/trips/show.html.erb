<div id="map-canvas"></div>
<div id='panel'>
  <div id='search-bar'>
    <input type="text" name="query" id="query"><button type="button" class="btn btn-info" id="search-button">Search</button>
  </div>
    <%= form_for @trip do |f| %>
      <%=f.text_field :start_from ,:id=>"origin_field" %>
      <%=f.text_field :go_to ,:id=>"dest_field" %>
      <button type="button" class="btn btn-info" id="dir-button">Show Direction</button>
    <% end %>
</div>
<!-- <div id='attractions'></div> -->

<script type="text/javascript">
  google.maps.event.addDomListener(window, 'load', initialize);
</script>
